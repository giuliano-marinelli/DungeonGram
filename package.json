  "main": "main.js",
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node dist/server/app.js",
    "build": "concurrently -n \"client,server\" -c \"blue,yellow\" \"ng build --aot --prod\" \"tsc -p server\"",
    "predev": "concurrently -n \"server(build)\" -c \"yellow\" \"tsc -p server\"",
    "dev": "concurrently -n \"client,server(build),server\" -c \"blue,yellow,yellow\" \"ng build --watch\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "preprod": "concurrently -n \"client(build),server(build)\" -c \"blue,yellow\" \"ng build --aot --prod\" \"tsc -p server\"",
    "prod": "concurrently -n \"server\" -c \"yellow\" \"node dist/server/app.js\"",
    "bgserver": "pm2 start dist/server/app.js",
    "preelectron": "concurrently -n \"client(build),server(build)\" -c \"blue,yellow,magenta\" \"ng build --aot --prod\" \"tsc -p server\"",
    "electron": "concurrently -n \"electron,server,server\" -c \"blue,green,yellow,yellow\" \"electron .\" \"tsc -w -p server\" \"nodemon dist/server/app.js\""
  },
  "private": true,
