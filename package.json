  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "npm run prod",
    "predev": "concurrently -n \"server(build),gameserver(build)\" -c \"yellow,magenta\" \"tsc -p server\" \"tsc -p gameserver\"",
    "dev": "concurrently -n \"mongodb,client,server,gameserver,server,gameserver\" -c \"green,blue,yellow,magenta,yellow,magenta\" \"mongod\" \"ng build --watch\" \"tsc -w -p server\" \"tsc -w -p gameserver\" \"nodemon dist/server/app.js\" \"nodemon dist/gameserver/app.js\"",
    "preprod": "concurrently -n \"client(build),server(build),gameserver(build)\" -c \"blue,yellow,magenta\" \"ng build --aot --prod\" \"tsc -p server\" \"tsc -p gameserver\"",
    "prod": "concurrently -n \"mongodb,server,gameserver\" -c \"green,yellow,magenta\" \"mongod\" \"node dist/server/app.js\" \"node dist/gameserver/app.js\""
  },
  "private": true,
